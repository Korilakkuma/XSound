{
  "name": "xsound",
  "version": "2.10.1",
  "description": "Web Audio API Library for Synthesizer, Effects, Visualization, Multi-Track Recording ... etc",
  "main": "build/xsound.js",
  "scripts": {
    "clean": "rm -rf build",
    "lint": "eslint src",
    "build": "npm run clean && webpack --progress --mode production",
    "build:watch": "npm run clean && webpack --progress --watch --mode development",
    "test": "npm run lint && karma start",
    "docker-compose:build": "docker-compose build",
    "start": "docker-compose up -d",
    "stop": "docker stop xsound"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Korilakkuma/XSound.git"
  },
  "keywords": [
    "XSound",
    "JavaScript",
    "Web Audio API",
    "Web Audio",
    "Web Audio API Library",
    "Web Audio Library"
  ],
  "author": "Korilakkuma <rilakkuma.san.xjapan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Korilakkuma/XSound/issues"
  },
  "homepage": "https://korilakkuma.github.io/XSound/",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-cli": "^1.1.1",
    "jasmine": "^3.4.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-webpack": "^3.0.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  }
}
